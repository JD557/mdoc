{
  "name": "mdoc",
  "displayName": "mdoc",
  "description": "Markdown Scala Worksheets",
  "version": "0.0.1",
  "publisher": "mdoc",
  "engines": {
    "vscode": "^1.19.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension",
  "activationEvents": [],
  "contributes": {
    "markdown.markdownItPlugins": true
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/markdown-it": "0.0.4",
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  }
}
